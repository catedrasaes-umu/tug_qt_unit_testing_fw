    {
        ##TESTSUITEPARENTNAME##::finish_testsuite();
        
        /// >>>>>
##TESTSUITEFINISH##
        /// <<<<< 
    }

    ///
    /// Tests definition
    ///

private Q_SLOTS: /// (mandatory) Tests have to be declared as "private slots"

##TESTSDEFINITION##

protected:

    ///
